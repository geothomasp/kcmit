<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="DocumentAccess" parent="DocumentAccess-parentBean" />
    <bean id="DocumentAccess-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.coeus.common.framework.auth.docperm.DocumentAccess" />
        <property name="objectLabel" value="Document Access" />
        <property name="inquiryDefinition" >
            <ref bean="DocumentAccess-inquiryDefinition" />
        </property>
        <property name="lookupDefinition" >
            <ref bean="DocumentAccess-lookupDefinition" />
        </property>
        <property name="attributes" >
            <list>
                <ref bean="DocumentAccess-id" />
                <ref bean="DocumentAccess-documentNumber" />
                <ref bean="DocumentAccess-principalId" />
                <ref bean="DocumentAccess-roleName" />
                <ref bean="DocumentAccess-namespaceCode" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->

    <bean id="DocumentAccess-id" parent="DocumentAccess-id-parentBean" />
    <bean id="DocumentAccess-id-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="id" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Document Access Id" />
        <property name="shortLabel" value="Doc Access Id" />
        <property name="maxLength" value="12" />
        <property name="control">
            <bean parent="TextControlDefinition" p:size="14"/>
        </property>
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="14"/>
        </property>
        <property name="summary" value="Document Access Id" />
        <property name="description" value="Document Access Id" />
    </bean>

    <bean id="DocumentAccess-documentNumber" parent="DocumentAccess-documentNumber-parentBean"/>
    <bean id="DocumentAccess-documentNumber-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="forceUppercase" value="true"/>
        <property name="shortLabel" value="Doc Nbr"/>
        <property name="maxLength" value="14"/>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="16"/>
        </property>
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="16"/>
        </property>
        <property name="summary" value="Document Number"/>
        <property name="name" value="documentNumber"/>
        <property name="label" value="Document Number"/>
        <property name="description" value="The document id is generated by the workflow environment and is unique to each installation of Kuali..."/>
    </bean>

    <bean id="DocumentAccess-principalId" parent="DocumentAccess-principalId-parentBean" />
    <bean id="DocumentAccess-principalId-parentBean" parent="AttributeDefinition" abstract="true"
          p:name="principalId"
          p:forceUppercase="true"
          p:label="Principal ID"
          p:shortLabel="Principal ID"
          p:maxLength="40"
          p:required="true" >
        <property name="validationPattern" >
            <bean parent="AlphaNumericValidationPattern" />
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="15" />
        </property>
        <property name="controlField" >
            <bean parent="Uif-TextControl" p:size="15" />
        </property>
    </bean>

    <bean id="DocumentAccess-roleName" parent="DocumentAccess-roleName-parentBean"/>
    <bean id="DocumentAccess-roleName-parentBean" parent="AttributeDefinition" abstract="true" p:name="roleName" p:forceUppercase="false"
          p:label="Role Name" p:shortLabel="Role Name" p:maxLength="80" p:required="true">
        <property name="control">
            <bean parent="TextControlDefinition" p:size="30"/>
        </property>
        <property name="controlField">
            <bean parent="Uif-TextControl" p:size="30"/>
        </property>
    </bean>

    <bean id="DocumentAccess-namespaceCode" parent="DocumentAccess-namespaceCode-parentBean"/>
    <bean id="DocumentAccess-namespaceCode-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="name" value="namespaceCode"/>
        <property name="forceUppercase" value="true"/>
        <property name="label" value="Namespace Code"/>
        <property name="shortLabel" value="Namespace Cd"/>
        <property name="maxLength" value="20"/>
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern"/>
        </property>
        <property name="required" value="true"/>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="10"/>
        </property>
        <property name="summary" value="Code identifying the namespace"/>
        <property name="description" value="Code identifying the namespace"/>
    </bean>

    <!-- Business Object Inquiry Definition -->
    <bean id="DocumentAccess-inquiryDefinition" parent="DocumentAccess-inquiryDefinition-parentBean" />
    <bean id="DocumentAccess-InquiryView" parent="DocumentAccess-InquiryView-parentBean"/>
    <bean id="DocumentAccess-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Document Access" />
        <property name="inquirySections" >
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Document Access" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields" >
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="documentNumber"/>
                            <bean parent="FieldDefinition" p:attributeName="principalId" />
                            <bean parent="FieldDefinition" p:attributeName="roleName"/>
                            <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <bean id="DocumentAccess-InquiryView-parentBean" abstract="true" parent="Uif-InquiryView">
        <property name="headerText" value="Document Access"/>
        <property name="dataObjectClassName" value="org.kuali.coeus.common.framework.auth.docperm.DocumentAccess"/>
        <property name="items">
            <list>
                <bean parent="Uif-Disclosure-GridSection">
                    <property name="headerText" value="Document Access"/>
                    <property name="layoutManager.numberOfColumns" value="2"/>
                    <property name="items">
                        <list>
                            <bean p:propertyName="documentNumber" parent="Uif-DataField"/>
                            <bean p:propertyName="principalId" parent="Uif-DataField"/>
                            <bean p:propertyName="roleName" parent="Uif-DataField"/>
                            <bean p:propertyName="namespaceCode" parent="Uif-DataField"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->
    <bean id="DocumentAccess-lookupDefinition" parent="DocumentAccess-lookupDefinition-parentBean" />
    <bean id="DocumentAccess-LookupView" parent="DocumentAccess-LookupView-parentBean"/>
    <bean id="DocumentAccess-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="Document Access" />
        <property name="defaultSort" >
            <bean parent="SortDefinition">
                <property name="attributeNames">
                    <list>
                        <value>documentNumber</value>
                    </list>
                </property>
            </bean>
        </property>
        <property name="lookupFields" >
            <list>
                <bean parent="FieldDefinition" p:attributeName="documentNumber"/>
                <bean parent="FieldDefinition" p:attributeName="principalId" />
                <bean parent="FieldDefinition" p:attributeName="roleName"/>
                <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
            </list>
        </property>
        <property name="resultFields" >
            <list>
                <bean parent="FieldDefinition" p:attributeName="documentNumber"/>
                <bean parent="FieldDefinition" p:attributeName="principalId" />
                <bean parent="FieldDefinition" p:attributeName="roleName"/>
                <bean parent="FieldDefinition" p:attributeName="namespaceCode"/>
            </list>
        </property>
    </bean>

    <bean id="DocumentAccess-LookupView-parentBean" abstract="true" parent="Uif-LookupView">
        <property name="dataObjectClassName" value="org.kuali.coeus.common.framework.auth.docperm.DocumentAccess"/>
        <property name="headerText" value="Document Access"/>
        <property name="defaultSortAttributeNames">
            <list>
                <value>documentNumber</value>
            </list>
        </property>
        <property name="criteriaFields">
            <list>
                <bean p:propertyName="documentNumber" parent="Uif-LookupCriteriaInputField"/>
                <bean p:propertyName="principalId" parent="Uif-LookupCriteriaInputField"/>
                <bean p:propertyName="roleName" parent="Uif-LookupCriteriaInputField"/>
                <bean p:propertyName="namespaceCode" parent="Uif-LookupCriteriaInputField"/>
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean p:propertyName="documentNumber" parent="Uif-LookupCriteriaInputField"/>
                <bean p:propertyName="principalId" parent="Uif-LookupCriteriaInputField"/>
                <bean p:propertyName="roleName" parent="Uif-LookupCriteriaInputField"/>
                <bean p:propertyName="namespaceCode" parent="Uif-LookupCriteriaInputField"/>
            </list>
        </property>
    </bean>

</beans>
