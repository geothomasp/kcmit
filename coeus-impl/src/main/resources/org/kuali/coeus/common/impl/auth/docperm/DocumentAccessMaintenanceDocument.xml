<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="DocumentAccessMaintenanceDocument" parent="DocumentAccessMaintenanceDocument-parentBean"/>

  <bean id="DocumentAccessMaintenanceDocument-parentBean" abstract="true" parent="KcMaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.coeus.common.framework.auth.docperm.DocumentAccess"/>
    <property name="maintainableClass" value="org.kuali.kra.maintenance.KraMaintainableImpl"/>
    <property name="businessRulesClass" value="org.kuali.coeus.common.impl.auth.docperm.DocumentAccessRule" />
    <property name="maintainableSections">
      <list>
        <ref bean="DocumentAccessMaintenanceDocument-Edit"/>
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>id</value>
      </list>
    </property>
	<property name="allowsRecordDeletion" value="true" />

    <property name="documentTypeName" value="DocumentAccessMaintenanceDocument"/>
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="DocumentAccessMaintenanceDocument-Edit" parent="DocumentAccessMaintenanceDocument-Edit-parentBean"/>
  <bean id="DocumentAccessMaintenanceDocument-Edit-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Document Access"/>
    <property name="title" value="Edit Document Access"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="documentNumber" p:required="true" />
        <bean parent="MaintainableFieldDefinition" p:name="principalId" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="roleName" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="namespaceCode" p:required="true"/>
      </list>
    </property>
  </bean>
</beans>