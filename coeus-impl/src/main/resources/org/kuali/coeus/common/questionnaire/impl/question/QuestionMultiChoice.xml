<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="QuestionMultiChoice" parent="QuestionMultiChoice-parentBean" />
    <bean id="QuestionMultiChoice-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.coeus.common.questionnaire.framework.question.QuestionMultiChoice" />
		<property name="objectLabel" value="Question Multi-Choice" />
        <property name="inquiryDefinition" >
            <ref bean="QuestionMultiChoice-inquiryDefinition" />
        </property>
        <property name="lookupDefinition" >
            <ref bean="QuestionMultiChoice-lookupDefinition" />
        </property>
        <property name="titleAttribute" value="id" />
        <property name="attributes" >
            <list>
                <ref bean="QuestionMultiChoice-id" />
                <ref bean="QuestionMultiChoice-questionId" />
                <ref bean="QuestionMultiChoice-prompt" />
                <ref bean="QuestionMultiChoice-description" />
                <ref bean="QuestionMultiChoice-versionNumber" />
            </list>
        </property>
    </bean>

    <!-- Attribute Definitions -->
    <bean id="QuestionMultiChoice-id" parent="QuestionMultiChoice-id-parentBean" />
    <bean id="QuestionMultiChoice-id-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="id" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Question Multi-Choice Id" />
        <property name="shortLabel" value="Question Multi-Choice Id" />
        <property name="maxLength" value="12" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Question Multi-Choice Id" />
        <property name="description" value="Question Multi-Choice Id" />
    </bean>

    <bean id="QuestionMultiChoice-questionId" parent="QuestionMultiChoice-questionId-parentBean" />
    <bean id="QuestionMultiChoice-questionId-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="questionId" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Question Ref Id" />
        <property name="shortLabel" value="Question Ref Id" />
        <property name="maxLength" value="12" />
        <property name="validationPattern" >
            <bean parent="NumericValidationPattern" />
        </property>
    <property name="validCharactersConstraint">
      <bean parent="NumericPatternConstraint"/>
    </property>
        <property name="control" >
      <bean parent="TextControlDefinition" p:size="10"/>
    </property>
    <property name="controlField">
      <bean p:size="10" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Question Ref Id" />
        <property name="description" value="Question Ref Id" />
    </bean>

    <bean id="QuestionMultiChoice-prompt" parent="QuestionMultiChoice-prompt-parentBean" />
    <bean id="QuestionMultiChoice-prompt-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="prompt" />
        <property name="required" value="true" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Prompt" />
        <property name="shortLabel" value="Prompt" />
        <property name="maxLength" value="200" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" />
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint"/>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="40"/>
        </property>
        <property name="controlField">
            <bean p:size="40" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Prompt" />
        <property name="description" value="Prompt" />
    </bean>

    <bean id="QuestionMultiChoice-description" parent="QuestionMultiChoice-description-parentBean" />
    <bean id="QuestionMultiChoice-description-parentBean" abstract="true" parent="AttributeDefinition" >
        <property name="name" value="description" />
        <property name="required" value="true" />
        <property name="forceUppercase" value="false" />
        <property name="label" value="Description" />
        <property name="shortLabel" value="Description" />
        <property name="maxLength" value="200" />
        <property name="validationPattern" >
            <bean parent="AnyCharacterValidationPattern" />
        </property>
        <property name="validCharactersConstraint">
            <bean parent="AnyCharacterPatternConstraint"/>
        </property>
        <property name="control" >
            <bean parent="TextControlDefinition" p:size="40"/>
        </property>
        <property name="controlField">
            <bean p:size="40" parent="Uif-TextControl"/>
        </property>
        <property name="summary" value="Description" />
        <property name="description" value="Description" />
    </bean>
    
      <bean id="QuestionMultiChoice-versionNumber" parent="QuestionMultiChoice-versionNumber-parentBean" />
      <bean id="QuestionMultiChoice-versionNumber-parentBean" abstract="true" parent="AttributeReferenceDummy-versionNumber"/>
        <!-- Business Object Inquiry Definition -->
      <bean id="QuestionMultiChoice-inquiryDefinition" parent="QuestionMultiChoice-inquiryDefinition-parentBean" />
      <bean id="QuestionMultiChoice-InquiryView" parent="QuestionMultiChoice-InquiryView-parentBean"/>
    <bean id="QuestionMultiChoice-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Question Multi-Choice Id" />
        <property name="inquirySections" >
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Question Multi-Choice Id" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields" >
                        <list>
              <bean parent="FieldDefinition" p:attributeName="id" p:forceInquiry="true"/>
              <bean parent="FieldDefinition" p:attributeName="questionId"/>
              <bean parent="FieldDefinition" p:attributeName="prompt"/>
              <bean parent="FieldDefinition" p:attributeName="description"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

  <bean id="QuestionMultiChoice-InquiryView-parentBean" abstract="true" parent="Uif-InquiryView">
    <property name="headerText" value="Question Multi-Choice Id"/>
    <property name="dataObjectClassName" value="org.kuali.coeus.common.questionnaire.framework.question.QuestionMultiChoice"/>
    <property name="items">
      <list>
        <bean parent="Uif-Disclosure-GridSection">
          <property name="headerText" value="Question Multi-Choice Id"/>
          <property name="layoutManager.numberOfColumns" value="2"/>
          <property name="items">
            <list>
              <bean p:propertyName="id" parent="Uif-DataField"/>
              <bean p:propertyName="questionId" parent="Uif-DataField"/>
              <bean p:propertyName="prompt" parent="Uif-DataField"/>
              <bean p:propertyName="description" parent="Uif-DataField"/>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

    <!-- Business Object Lookup Definition -->
    <bean id="QuestionMultiChoice-lookupDefinition" parent="QuestionMultiChoice-lookupDefinition-parentBean" />
  <bean id="QuestionMultiChoice-LookupView" parent="QuestionMultiChoice-LookupView-parentBean"/>
    <bean id="QuestionMultiChoice-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="title" value="Question Multi-Choice Lookup" /><property name="defaultSort" >
      <bean parent="SortDefinition"/>
        </property>
        <property name="lookupFields" >
            <list>
        <bean parent="FieldDefinition" p:attributeName="id" p:forceInquiry="true"/>
        <bean parent="FieldDefinition" p:attributeName="questionId"/>
        <bean parent="FieldDefinition" p:attributeName="prompt"/>
        <bean parent="FieldDefinition" p:attributeName="description"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="id" p:forceInquiry="true"/>
        <bean parent="FieldDefinition" p:attributeName="questionId"/>
        <bean parent="FieldDefinition" p:attributeName="prompt"/>
        <bean parent="FieldDefinition" p:attributeName="description"/>
      </list>
    </property>
  </bean>

  <bean id="QuestionMultiChoice-LookupView-parentBean" abstract="true" parent="Uif-LookupView">
    <property name="dataObjectClassName" value="org.kuali.coeus.common.questionnaire.framework.question.QuestionMultiChoice"/>
    <property name="headerText" value="Question Multi-Choice Lookup"/>
    <property name="criteriaFields">
      <list>
        <bean p:propertyName="id" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="questionId" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="prompt" parent="Uif-LookupCriteriaInputField"/>
        <bean p:propertyName="description" parent="Uif-LookupCriteriaInputField"/>
            </list>
        </property>
        <property name="resultFields" >
            <list>
        <bean p:propertyName="id" parent="Uif-DataField"/>
        <bean p:propertyName="questionId" parent="Uif-DataField"/>
        <bean p:propertyName="prompt" parent="Uif-DataField"/>
        <bean p:propertyName="description" parent="Uif-DataField"/>
            </list>
        </property>
    </bean>
</beans>
